{
	"name": "jsoop",
	"version": "1.0.0",
	"description": "practice project",
	"main": "index.js",
	"scripts": {
		"watch:sass": "node-sass style.scss style.css -w",
		"dev-server": "live-server",
		"start": "npm-run-all --parallel dev-server watch:sass",
